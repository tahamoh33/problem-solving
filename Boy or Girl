#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main(){
string username;
cin>>username;
int counter=0;
sort(username.begin(),username.end());
for(int i=0;i<username.size();i++){
    if(username[i]!=username[i+1]){
        counter++;
    }
}
 
if(counter%2==0){
    cout<<"CHAT WITH HER!";
 
}else if(counter%2 !=0){
 
cout<<"IGNORE HIM!";
 
}
 
 
 
 
 
 
 
 
 
 
    return 0;
}
